{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.base.js"
      }
    }
  },
  "rules": {
    "comma-dangle": 0,
    "react/prefer-stateless-function": 0,
    "max-len": [1, 120, 2, {"ignoreComments": true}]
  }
}
